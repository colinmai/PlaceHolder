<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_physics_class"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.xterstar.www.placeholder.physicsClass">

    <TextView
        android:text="Physics"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/textView10"
        tools:textSize="36sp"
        android:textStyle="normal|bold" />

    <Button
        android:text="Physics 1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:id="@+id/button33"
        android:layout_below="@+id/textView10"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Physics 3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button34"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/button35" />

    <Button
        android:text="Physics 4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button33"
        android:layout_alignRight="@+id/button33"
        android:layout_alignEnd="@+id/button33"
        android:layout_marginTop="26dp"
        android:id="@+id/button36" />

    <TextView
        android:text="More to be added soon..."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="16dp"
        android:id="@+id/textView11"
        tools:textSize="50sp" />

    <Button
        android:text="Physics 2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button34"
        android:layout_alignBaseline="@+id/button33"
        android:layout_alignBottom="@+id/button33"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="Physics 6a"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button37"
        android:layout_alignBaseline="@+id/button36"
        android:layout_alignBottom="@+id/button36"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="Physics 6b"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button38"
        android:layout_alignBaseline="@+id/button37"
        android:layout_alignBottom="@+id/button37"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

</RelativeLayout>
